[
    {
        "kernelSource": {
        "name": "android_gki_kernel_5.15_common",
        "repo": "https://github.com/hfdem/android_gki_kernel_5.15_common.git",
        "branch": "android13-5.15-2025-03",
        "device": "gki",
        "defconfig": "gki_defconfig"
        },
        "toolchains": [
            {
                "name": "clang",
                "repo": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86",
                "binaryEnv": ["./bin"]
            }
        ],
        "enableCcache": true,
        "params": {
            "ARCH": "arm64",
            "CC": "clang",
            "externalCommands": {
                "CLANG_TRIPLE": "aarch64-linux-gnu-",
                "CROSS_COMPILE": "clang/bin/aarch64-linux-gnu-",
                "CROSS_COMPILE_ARM32": "clang/bin/arm-linux-gnueabi-",
                "AS": "clang/bin/llvm-as", 
                "AR": "clang/bin/llvm-ar",  
                "NM":"clang/bin/llvm-nm" ,  
                "OBJCOPY":"clang/bin/llvm-objcopy",  
                "OBJDUMP":"clang/bin/llvm-objdump",  
                "STRIP":"clang/bin/llvm-strip",  
                "LD":"clang/bin/ld.lld", 
                "LTO": "thin"
            }
        },
        "AnyKernel3": {
            "use": true,
            "release": true
        },
        "enableKernelSU": true
    }
]